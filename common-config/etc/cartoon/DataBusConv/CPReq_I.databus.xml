<?xml version="1.0" encoding="UTF-8"?>
<DataConvMap xmlns="http://www.legend.com/DataConvMap.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.legend.com/DataConvMap.xsd DataConvMap.xsd">
	<DataConv>
		<Source>cartoon.message.id</Source>
		<Destination>MESSAGEID</Destination>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.version</Source>
		<Destination>VERSION</Destination>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.instid</Source>
		<Destination>INSTID</Destination>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.certid</Source>
		<Destination>CERTID</Destination>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.serialno</Source>
		<Destination>SERIALNO</Destination>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.date</Source>
		<Destination>DATE</Destination>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.signno</Source>
		<Destination>SIGNNO</Destination>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.charge</Source>
		<Destination>CHARGE</Destination>
		<Format dec="2">point</Format>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.amount</Source>
		<Destination>AMOUNT</Destination>
		<Format dec="2">point</Format>
	</DataConv>
	<DataConv>
		<Source>cartoon.message.cpreq.currency</Source>
		<Destination>CURRENCY</Destination>
	</DataConv>
</DataConvMap>